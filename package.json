{
  "name": "tuimao-config",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "目前包含 Vue3 Eslint 预设，tailwind 预设。\r 使用文档暂未开发，请自行运量使用。",
  "scripts": {
    "build": "esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "publish": "lerna publish --contents dist",
    "bootstrap": "lerna bootstrap",
    "update": "esno scripts/update.ts",
    "clean": "rimraf dist packages/*/dist/**/*.[jt]s",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuiMao233/tuimao-config.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TuiMao233/tuimao-config/issues"
  },
  "homepage": "https://github.com/TuiMao233/tuimao-config#readme",
  "devDependencies": {
    "@dcloudio/types": "^2.4.5",
    "@tuimao/eslint-config-preset": "^1.1.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.4.12",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "miniprogram-api-typings": "^3.4.2",
    "prettier": "^2.3.2",
    "vue": "^3.1.5"
  },
  "dependencies": {
    "@tuimao/axios": "file:packages\\axios",
    "@tuimao/browser": "file:packages\\browser",
    "@tuimao/core": "file:packages\\core",
    "@tuimao/eslint": "file:packages\\_eslint",
    "@tuimao/tailwind": "file:packages\\_tailwind",
    "@tuimao/uni-use": "file:packages\\uni-use",
    "@tuimao/uni-utils": "file:packages\\uni-utils",
    "@tuimao/utils": "file:packages\\utils",
    "@tuimao/vue-directive": "file:packages\\vue-directive",
    "@tuimao/vue-use": "file:packages\\vue-use",
    "@tuimao/vue-utils": "file:packages\\vue-utils"
  }
